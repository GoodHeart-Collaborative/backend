		"@types/chai-http": "^3.0.5",
		"@types/mocha": "^5.2.7",
		"@types/mongoose": "^5.7.32",
		"@types/node": "^10.14.22",
		"amqplib": "^0.5.5",
		"atob": "^2.1.2",
		"aws-sdk": "^2.553.0",
		"bcrypt": "^3.0.6",
		"boom": "^7.3.0",
		"btoa": "^1.2.1",
		"bull": "^3.11.0",
