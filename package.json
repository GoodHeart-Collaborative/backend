{
	"name": "good_heart",
	"version": "1.0.0",
	"description": "Node backend in Hapi.js and TypeScript.",
	"main": "index.js",
	"scripts": {
		"prestart": "tsc && cp ./google_auth.json ./build/",
		"local": "tsc && cp ./google_auth.json ./build/ && NODE_ENV=local node ./build/server.js",
		"local:windows": " tsc && cp ./google_auth.json ./build/ && set NODE_ENV=local && node ./build/server.js",
		"start": "NODE_ENV=local nodemon --exec ts-node -- server.ts",
		"start:windows": "set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
		"local:ts-node": "NODE_ENV=local ts-node -r tsconfig-paths/register server.ts --fast",
		"local:ts-node-dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register server.ts --fast",
		"dev1": "tsc  && cp ./google_auth.json ./build/ && NODE_ENV=development node ./build/server.js",
		"test": "tsc && cp ./google_auth.json ./build/ && NODE_ENV=testing node ./build/server.js",
		"stag": "tsc && cp ./google_auth.json ./build/ && NODE_ENV=staging node ./build/server.js",
		"beta": "tsc  && cp ./google_auth.json ./build/ && NODE_ENV=testing node ./build/server.js",
		"dev": "nodemon server.ts",
		"dev:debug": "nodemon --inspect server.ts",
		"pm2:dev": "tsc && cp ./google_auth.json ./build/ && NODE_ENV=development pm2 start ./build/server.js --name good_heart-dev",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"tslint": "tslint --project tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "http://gitlab.appinvent.in/women-community-app/backend.git"
	},
	"author": "Appinventiv Technologies",
	"license": "ISC",
	"dependencies": {
		"@types/chai-http": "^3.0.5",
		"@types/mocha": "^5.2.7",
		"@types/mongoose": "^5.7.32",
		"@types/node": "^10.14.22",
		"atob": "^2.1.2",
		"aws-sdk": "^2.553.0",
		"bcrypt": "^3.0.6",
		"bluebird": "^3.7.2",
		"boom": "^7.3.0",
		"btoa": "^1.2.1",
		"bunyan": "^1.8.12",
		"bunyan-debug-stream": "^2.0.0",
		"bunyan-rotating-file-stream": "^1.6.3",
		"chai-http": "^4.3.0",
		"debug": "^4.1.1",
		"del": "^3.0.0",
		"dev1": "^1.0.1",
		"dotenv": "^6.2.0",
		"fcm-node": "^1.5.2",
		"generate-password": "^1.4.2",
		"geoip-lite": "^1.4.2",
		"good": "^8.1.2",
		"good-console": "^7.1.0",
		"googleapis": "^67.0.0",
		"hapi": "^18.1.0",
		"hapi-auth-basic": "^5.0.0",
		"hapi-auth-bearer-token": "^6.2.1",
		"hapi-swagger": "^9.4.2",
		"inert": "^5.1.3",
		"joi": "^14.3.1",
		"jsonwebtoken": "^8.5.1",
		"jsonwebtoken-refresh": "^5.7.0",
		"lodash": "^4.17.15",
		"md5": "^2.2.1",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"mongoose": "^5.9.25",
		"node-cron": "^2.0.3",
		"nodemailer": "^5.1.1",
		"randomstring": "^1.1.5",
		"request": "^2.88.0",
		"request-promise": "^4.2.6",
		"run": "^1.4.0",
		"shortid": "^2.2.15",
		"sns-mobile": "^1.3.0",
		"tinyurl": "^1.1.7",
		"validator": "^10.11.0",
		"vision": "^5.4.4"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"codelyzer": "^4.5.0",
		"mocha": "^5.2.0",
		"ts-node": "^8.4.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.6.4",
		"typescript-tools": "^0.3.1"
	},
	"_moduleAliases": {
		"@root": "./build",
		"@utils": "./build/src/utils/",
		"@lib": "./build/src/lib/",
		"@base-dao": "./build/src/baseDao",
		"@config": "./build/src/config",
		"@exceptions": "./build/src/exceptions",
		"@interfaces": "./build/src/interfaces",
		"@plugins": "./build/src/plugins",
		"@routes": "./build/src/routes",
		"@modules": "./build/src/modules"
	}
}
