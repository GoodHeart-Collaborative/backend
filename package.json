{
	"name": "node_rcc",
	"version": "1.0.0",
	"description": "Node backend in Hapi.js and TypeScript.",
	"main": "index.js",
	"scripts": {
		"prestart": "tsc",
		"local": "tsc && NODE_ENV=local node ./build/server.js",
		"local:windows": " tsc && set NODE_ENV=local && node ./build/server.js",
		"start": "NODE_ENV=local nodemon --exec ts-node -- server.ts",
		"start:windows": "set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
		"local:ts-node": "NODE_ENV=local ts-node -r tsconfig-paths/register server.ts --fast",
		"local:ts-node-dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register server.ts --fast",
		"dev1": "tsc && NODE_ENV=development node ./build/server.js",
		"test": "tsc && NODE_ENV=testing node ./build/server.js",
		"stag": "tsc && NODE_ENV=staging node ./build/server.js",
		"dev": "nodemon server.ts",
		"dev:debug": "nodemon --inspect server.ts",
		"pm2:dev": "tsc && NODE_ENV=development pm2 start ./build/server.js --name rcc-dev",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"tslint": "tslint --project tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "http://gitlab.appinvent.in/NodeRCC/Backend.git"
	},
	"author": "Appinventiv Technologies",
	"license": "ISC",
